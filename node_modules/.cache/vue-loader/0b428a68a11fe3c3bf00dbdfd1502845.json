if=\"selectItem == '首页'\"\n        :style=\"{ margin: '16px 0' }\"\n      >\n        <a-breadcrumb-item\n          :style=\"{\n            'background-color': '#80bd01',\n            color: '#fff',\n            'border-radius': '3px',\n            padding: '3px 4px',\n            margin: '10px 10px'\n          }\"\n          :key=\"i\"\n          v-for=\"(sub, i) in subList\"\n          >{{ sub }}</a-breadcrumb-item\n        >\n      </a-breadcrumb>\n      <a-breadcrumb v-else :style=\"{ margin: '16px 0' }\">\n        <a-breadcrumb-item>主页</a-breadcrumb-item>\n        <a-breadcrumb-item>{{ selectItem }}</a-breadcrumb-item>\n      </a-breadcrumb>\n      <div :style=\"{ background: '#fff', padding: '24px', minHeight: '380px' }\">\n        Content\n      </div>\n    </a-layout-content>\n    <a-layout-footer :style=\"{ textAlign: 'center' }\">\n      <Footer></Footer>\n    </a-layout-footer>\n  </a-layout>\n</template>\n<style>\n#components-layout-demo-fixed .logo {\n  width: 120px;\n  height: 31px;\n  margin: 16px 24px 16px 0;\n  float: left;\n  background-image: url(\"//static2.cnodejs.org/public/images/cnodejs_light.svg\");\n}\n</style>\n\n<script>\nimport Footer from \"@/layouts/footer.vue\";\nexport default {\n  data() {\n    return {\n      menuList: [\"首页\", \"新手入门\", \"API\", \"关于\", \"注册\", \"登录\"],\n      subList: [\"全部\", \"精华\", \"分享\", \"问答\", \"招聘\", \"客户端测试\"],\n      selectItem: \"首页\"\n    };\n  },\n  methods: {\n    changeMenu(item) {\n      this.selectItem = item;\n    }\n  },\n  components: {\n    Footer\n  }\n};\n</script>\n"]}]}