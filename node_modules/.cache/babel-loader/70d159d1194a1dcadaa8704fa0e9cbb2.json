AAA,QAAA,CAAA,IAAA;AACA,KANA,WAOA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,KAVA,aAWA;AAAA,aAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AAAA,KAXA;AAYA,GA7BA;AA8BA,EAAA,OAAA,EAAA;AACA,IAAA,UAAA,EAAA,oBAAA,KAAA,EAAA;AACA,aAAA,WAAA,CAAA,KAAA,CAAA;AACA;AAHA,GA9BA;AAmCA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA;AACA;AAHA;AAnCA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <a-breadcrumb separator=\" \" :style=\"{ margin: '16px 0' }\">\n      <a-breadcrumb-item\n        :style=\"{\n          'background-color': '#80bd01',\n          color: '#fff',\n          'border-radius': '3px',\n          padding: '3px 4px',\n          margin: '10px 10px'\n        }\"\n        :key=\"i\"\n        v-for=\"(route, i) in routes\"\n      >\n        <router-link :to=\"`?tag=${route.path}`\" @click.native=\"refresh\">\n          {{ route.breadcrumbName }}\n        </router-link>\n      </a-breadcrumb-item>\n    </a-breadcrumb>\n\n    <div class=\"cell\" v-for=\"(item, i) in all.data\" :key=\"i\">\n      <div class=\"left\">\n        <a :href=\"'/user/' + item.author.loginname\">\n          <img :src=\"item.author.avatar_url\" :title=\"item.author.loginname\" />\n        </a>\n        <span class=\"count_of_replies\" title=\"回复数\">\n          {{ item.reply_count }}/{{ item.visit_count }}\n        </span>\n\n        <span v-if=\"item.top\" class=\"put_top\">置顶</span>\n        <span v-if=\"item.good\" class=\"put_good\">精华</span>\n        <router-link\n          class=\"topic_title\"\n          :title=\"item.title\"\n          :to=\"{ path: '/topic/' + item.id }\"\n        >\n          {{ item.title }}\n        </router-link>\n      </div>\n      <div class=\"right\">\n        <span>\n          <a\n            class=\"last_time pull-right\"\n            href=\"/topic/5e0a0992ba8f6d46c4edfeb1#5e0f090a81adfe2602079c0a\"\n          >\n            <img\n              class=\"user_small_avatar\"\n              src=\"https://avatars0.githubusercontent.com/u/25097125?v=4&amp;s=120\"\n            />\n          </a>\n        </span>\n        <span class=\"last_active_time\">{{ item.create_at | formatDate }}</span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<style lang=\"less\" scoped>\n.left {\n  display: inline-block;\n  width: 70%;\n}\n\n.right {\n  width: 10%;\n  float: right;\n}\n\n.cell {\n  width: 80%;\n  margin: 10px;\n  border-top-width: 1px;\n  border-top-style: solid;\n  border-top-color: #ffffff;\n}\n\nimg {\n  width: 30px;\n  height: 30px;\n}\ndiv {\n  color: #333;\n}\n.cell:hover {\n  background-color: #e1e1e1;\n}\n.put_good,\n.put_top {\n  background: #80bd01;\n  padding: 2px 4px;\n  border-radius: 3px;\n  color: #fff;\n  font-size: 12px;\n}\n</style>\n\n<script>\nimport axios from \"axios\";\nimport formatDate from \"@/utils/formatData.js\";\nexport default {\n  data() {\n    console.log(\"nav is:\" + this.nav);\n    return {\n      all: [],\n      routes: [\n        { breadcrumbName: \"全部\", path: \"all\" },\n        { breadcrumbName: \"精华\", path: \"good\" },\n        { breadcrumbName: \"分享\", path: \"share\" },\n        { breadcrumbName: \"问答\", path: \"ask\" },\n        { breadcrumbName: \"招聘\", path: \"job\" },\n        { breadcrumbName: \"客户端测试\", path: \"dev\" }\n      ],\n      nav: \"all\"\n    };\n  },\n  mounted() {\n    axios\n      .get(\n        \"https://cnodejs.org/api/v1/topics?page=1&&limit=10&&tab=\" + this.nav\n      )\n      .then(response => {\n        this.all = response.data;\n      })\n      .catch(error => {\n        console.log(error);\n        this.errored = true;\n      })\n      .finally(() => (this.loading = false));\n  },\n  filters: {\n    formatDate: function(value) {\n      return formatDate(value);\n    }\n  },\n  methods: {\n    refresh() {\n      this.$router.go(0);\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views"}]}