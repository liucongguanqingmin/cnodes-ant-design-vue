,KAAA,EAAA,CAAA,KAAA,CAPA;AAQA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AAHA,GARA;AAaA,EAAA,OAbA,qBAaA;AAAA;;AACA,IAAA,KAAA,CACA,GADA,CAEA,6DAAA,KAAA,GAFA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,GAAA,GAAA,QAAA,CAAA,IAAA;AACA,KANA,WAOA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,KAVA,aAWA;AAAA,aAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AAAA,KAXA;AAYA,GA1BA;AA2BA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,KADA,EACA;AACA;AACA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,GAAA,KAAA,IAAA,IAAA,CAAA,KAAA,CAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,CAAA,GAAA,IAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA;;AACA,YAAA,CAAA,IAAA,CAAA,EAAA;AACA,iBAAA,IAAA,CAAA,KAAA,CAAA,CAAA,IAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA,GAAA,GAAA;AACA;AACA;AACA;AAXA;AA3BA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"cell\" v-for=\"(item, i) in all.data\" :key=\"i\">\n      <div class=\"left\">\n        <a :href=\"'/user/' + item.author.loginname\">\n          <img :src=\"item.author.avatar_url\" :title=\"item.author.loginname\" />\n        </a>\n        <span class=\"count_of_replies\" title=\"回复数\">\n          {{ item.reply_count }}/{{ item.visit_count }}\n        </span>\n\n        <span v-if=\"item.top\" class=\"put_top\">置顶</span>\n        <span v-if=\"item.good\" class=\"put_good\">精华</span>\n\n        <a class=\"topic_title\" :title=\"item.title\" @click=\"handleTopic\">\n          {{ item.title }}\n        </a>\n      </div>\n      <div class=\"right\">\n        <span>\n          <a\n            class=\"last_time pull-right\"\n            href=\"/topic/5e0a0992ba8f6d46c4edfeb1#5e0f090a81adfe2602079c0a\"\n          >\n            <img\n              class=\"user_small_avatar\"\n              src=\"https://avatars0.githubusercontent.com/u/25097125?v=4&amp;s=120\"\n            />\n          </a>\n        </span>\n        <span class=\"last_active_time\">{{ item.create_at | formatDate }}</span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<style lang=\"less\" scoped>\n.left {\n  display: inline-block;\n  width: 70%;\n}\n\n.right {\n  width: 10%;\n  float: right;\n}\n\n.cell {\n  width: 80%;\n  margin: 10px;\n  border-top-width: 1px;\n  border-top-style: solid;\n  border-top-color: #ffffff;\n}\n\nimg {\n  width: 30px;\n  height: 30px;\n}\ndiv {\n  color: #333;\n}\n.cell:hover {\n  background-color: #e1e1e1;\n}\n.put_good,\n.put_top {\n  background: #80bd01;\n  padding: 2px 4px;\n  border-radius: 3px;\n  color: #fff;\n  font-size: 12px;\n}\n</style>\n\n<script>\nimport axios from \"axios\";\nconst s = 1000,\n  m = 60 * s,\n  h = 60 * m,\n  d = 24 * h,\n  mo = 30 * d,\n  y = 365 * d;\n\nconst fdate = [\n  {\n    time: y,\n    text: \"年\"\n  },\n  {\n    time: mo,\n    text: \"月\"\n  },\n  {\n    time: d,\n    text: \"天\"\n  },\n  {\n    time: h,\n    text: \"小时\"\n  },\n  {\n    time: m,\n    text: \"分钟\"\n  },\n  {\n    time: s,\n    text: \"秒\"\n  }\n];\nexport default {\n  data() {\n    console.log(\"nav is:\" + this.nav);\n    return {\n      all: []\n    };\n  },\n  props: [\"nav\"],\n  methods: {\n    handleTopic() {\n      this.$router.push(\"/topic\");\n    }\n  },\n  mounted() {\n    axios\n      .get(\n        \"https://cnodejs.org/api/v1/topics?page=1&&limit=10&&tab=\" + this.nav\n      )\n      .then(response => {\n        this.all = response.data;\n      })\n      .catch(error => {\n        console.log(error);\n        this.errored = true;\n      })\n      .finally(() => (this.loading = false));\n  },\n  filters: {\n    formatDate(value) {\n      // s m h d mo y\n      // 计算出时间差\n      const time = Date.now() - new Date(value);\n      for (let i = 0; i < fdate.length; i++) {\n        const t = time / fdate[i].time;\n        if (t >= 1) {\n          return Math.floor(t) + fdate[i].text + \"前\";\n        }\n      }\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/layouts"}]}